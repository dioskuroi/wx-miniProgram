.goods-wrapper {
  width: calc(100vw - 60rpx);
  margin: 0 auto;
  border-bottom: 1rpx solid #ccc;
  display: flex;
  justify-content: space-between;
  height: 300rpx;
  padding: 20rpx 0;
}

.goods-img-wrapper {
  width: 260rpx;
  height: 260rpx;
  position: relative;
}

.goods-label {
  position: absolute;
  top: 0;
  left: 10rpx;
  background: linear-gradient(to bottom, deeppink, purple);
  width: 60rpx;
  height: 40rpx;
  border-radius: 0 0 10rpx 10rpx;
  text-align: center;
  line-height: 36rpx;
}

.font10 {
  transform: scale(0.9);
}

.label-content {
  color: #fff;
  font-size: 24rpx;
}

.goods-info {
  margin-left: 20rpx;
  flex-grow: 1;
  display: flex;
  flex-direction: column;
}

.goods-title {
  font-size: 32rpx;
  padding-top: 20rpx;
  flex: 6;
}

.goods-quantity {
  flex: 2;
  font-size: 24rpx;
  display: flex;
  justify-content: flex-start;
}

.icon-huo {
  color: blueviolet;
  font-size: 24rpx;
}

.sold {
  color: blueviolet;
}

.rest {
  color: #999;
}

.bottom {
  flex: 4;
  display: flex;
  justify-content: space-between;
}

.price {
  display: flex;
  line-height: 32rpx;
  padding-top: 40rpx;
}

.current {
  font-size: 32rpx;
  color: #f10215;
}

.prev {
  padding-top: 4rpx;
  font-size: 24rpx;
  text-decoration: line-through;
  margin-left: 10rpx;
}

