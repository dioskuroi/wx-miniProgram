  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1020326_j4fpa5lbwl.woff?t=1547706180979') format('woff'),
  url('//at.alicdn.com/t/font_1020326_j4fpa5lbwl.ttf?t=1547706180979') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
